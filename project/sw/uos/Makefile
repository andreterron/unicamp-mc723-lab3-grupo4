CC = mips-elf-gcc
AR = mips-elf-ar
CFLAGS = -msoft-float -specs=archc

TARGET = uos
INC_DIR := -I.

SRCS := uos.c
OBJS := $(SRCS:.c=.o)

#------------------------------------------------------
.SILENT:

#------------------------------------------------------
.SUFFIXES: .o .c .h

#------------------------------------------------------
all: $(OBJS)
	$(AR) -r lib$(TARGET).a $(OBJS)
#------------------------------------------------------
clean:
	rm -f *.o *~ lib$(TARGET).a
#------------------------------------------------------
distclean: clean
#------------------------------------------------------
.c.o:
	$(CC) -c $(CFLAGS) $(INC_DIR) -c $<

